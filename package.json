{
  "name": "stock-tracker-ts",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node dist/server.js",
    "start:dev": "concurrently \"nodemon --exec ts-node -T src/server.ts --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "tsc -p . && cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "jest  --maxWorkers=1 --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doanja/Stock-Tracker-TS.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doanja/Stock-Tracker-TS/issues"
  },
  "homepage": "https://github.com/doanja/Stock-Tracker-TS#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.30",
    "@types/node": "^14.0.14",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.25",
    "@types/supertest": "^2.0.10",
    "@types/yup": "^0.29.3",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.21",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "redis": "^3.0.2",
    "yup": "^0.29.3"
  }
}
